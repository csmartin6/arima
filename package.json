{
  "name": "arima",
  "version": "0.3.0",
  "description": "ARIMA, SARIMA, SARIMAX and AutoARIMA models for time series analysis and forecasting",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf wasm && rimraf wrapper",
    "mkdirs": "mkdir wasm && mkdir wrapper",
    "prepare-wasm": "jsmaker wasm/native.wasm -t uint8 > wrapper/native.bin.js",
    "test": "tape test.js | faucet",
    "build": "npm run clean && npm run mkdirs && make build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zemlyansky/arima"
  },
  "keywords": [
    "arima",
    "sarima",
    "sarimax",
    "autoarima",
    "timeseries",
    "prediction",
    "forecasting",
    "statistics",
    "webassembly",
    "wasm"
  ],
  "devDependencies": {
    "faucet": "0.0.1",
    "jsmaker": "0.0.2",
    "rimraf": "^3.0.2",
    "tape": "^5.0.1"
  },
  "author": "Anton Zemlyansky",
  "license": "MIT",
  "dependencies": {}
}
