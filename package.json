{
  "name": "arima",
  "version": "0.3.0",
  "description": "ARIMA, SARIMA, SARIMAX and AutoARIMA models for time series analysis and forecasting",
  "main": "index.js",
  "scripts": {
    "mkdirs": "mkdir src/wasm && mkdir src/wrapper",
    "test": "npx tsc && jest",
    "build": "npm run clean && npm run mkdirs && make buildWasm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zemlyansky/arima"
  },
  "keywords": [
    "arima",
    "sarima",
    "sarimax",
    "autoarima",
    "timeseries",
    "prediction",
    "forecasting",
    "statistics",
    "webassembly",
    "wasm"
  ],
  "dependencies": {
    "@types/emscripten": "^1.39.5",
    "lodash-es": "^4.17.15",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.5",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.3",
    "faucet": "0.0.1",
    "jest": "^27.0.6",
    "jsmaker": "0.0.2",
    "rimraf": "^3.0.2",
    "tape": "^5.0.1",
    "ts-jest": "^27.0.3"
  },
  "author": "Chris Martin",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
